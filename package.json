{
  "name": "sgroup-exercise-1",
  "version": "1.0.0",
  "description": "setup basic project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "dev": "nodemon --exec babel-node src/bin/www.js",
    "start": "npm run build && node ./dist/bin/www.js",
    "clean": "rm -rf dist && mkdir dist",
    "build:babel": "babel ./src --out-dir dist --copy-files",
    "build": "yarn run clean && yarn run build:babel",
    "knex": "node_modules/.bin/knex --knexfile ./knexfile.js",
    "db:reset": "yarn knex migrate:rollback && yarn knex migrate:latest && yarn knex seed:run",
    "create:migration": "yarn knex migrate:make",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "migrate:reset": "yarn run migrate:rollback && yarn run migrate:latest && yarn run seed:run",
    "seed:run": "yarn knex seed:run",
    "heroku-postbuild": "yarn install && yarn migrate:reset && yarn run build"
  },
  "repository": "git+https://github.com/kimchilee11/Sgroup-exercise-1.git",
  "author": "Kim Chi <kimchilethi411@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kimchilee11/Sgroup-exercise-1/issues"
  },
  "homepage": "https://github.com/kimchilee11/Sgroup-exercise-1#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "pug": "^3.0.2",
    "type": "module",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.14.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-xo": "^0.37.0",
    "eslint-plugin-import": "^2.21.2",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7"
  }
}
